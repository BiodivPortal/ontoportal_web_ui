= turbo_frame_tag "metadata" do  
  - length = (@metadata_sel.length) 
  %table.zebra{:cellpadding => "0", :cellspacing => "0"}
    %col
    %colgroup{:span=>"1"}
    %tr
      %td{:rowspan=>"1"}
      %td{:rowspan=>"1"}
      %th{:colspan=>length, :scope=>"colgroup", style:'text-align:center;'} Metadata
    %tr
      %th{style:'text-align:center;'} Acronyms
      %th{style:'text-align:center;'} Ontologies
      - @metadata_sel.each do |meta|
        %td{style:'text-align:center;'}= meta
      - (0..@ontologies.length - 1).each do |n|       
        %tr
          %td{style:'text-align:center;'}= @ontologies[n].acronym
          %td{style:'text-align:center;'}= @ontologies[n].name
          - @metadata_sel.each do |meta|
            %td{style:'text-align:center;'}= @submissions[n].instance_values[meta.to_s]  
